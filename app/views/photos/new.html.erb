<h1>Upload a Photo!</h1>

<%= form_for @photo, url: user_photos_url do |f| %>

  <%= f.label :Title %>:
  <%= f.text_field :title %><br />

  <%= f.label :Description %>:
  <%= f.text_field :description %><br />

  <%= f.label :Latitude %>:
  <%= f.text_field :latitude %><br />

  <%= f.label :Longitude %>:
  <%= f.text_field :longitude %><br />

  <%= f.file_field :image %><br />

  <%= f.submit %>

<% end %>
