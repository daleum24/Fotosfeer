<% photos.each do |photo| %>
  <div class="images_index">
	<% if title != "My Images" %>  
	    <a href="/photos/<%= photo.id %>"><img src="<%= photo.image.url %>"></a>
	<% end %>
	
	<% if title == "My Images" %>
		<a href="/photos/<%= photo.id %>/edit"><img src="<%= photo.image.url %>"></a>
	<% end %>
  </div>
<% end %>